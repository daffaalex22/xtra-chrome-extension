{
  "manifest_version": 3,
  "name": "Reading Time",
  "description": "Add reading time to Chrome Extension docs",
  "version": "1.0",
  "action": {
    "default_popup": "default.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/chrome-docs.js"],
      "matches": [
        "https://developer.chrome.com/docs/extensions/*"
      ]
    }
  ]
}