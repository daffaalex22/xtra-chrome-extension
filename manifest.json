{
  "manifest_version": 3,
  "name": "Limited X",
  "description": "Minimalize the X.com website",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "scripting",
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "scripts/elementRemover.js"
      ],
      "matches": [
        "https://x.com/home*"
      ]
    }
  ]
}